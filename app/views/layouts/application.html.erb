<!DOCTYPE html>
<html>
	
	<head>
	  <title>Prelude</title>
	  <%= stylesheet_link_tag    "application", :media => "all" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	</head>
	
	<body>
		<%= render "layouts/header" %>
		<div class="container">
			<% flash.each do |name, message|%>
				<%= content_tag :div, message, class: "alert alert-#{name}" %>
			<% end %>
			<%= yield %>
		</div>
	</body>

</html>
